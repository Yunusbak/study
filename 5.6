def fib_sonlar_to_n(n: int):
    fib_sonlar = [0,1]
    while fib_sonlar[-1] + fib_sonlar[-2] <= n:
        fib_sonlar.append(fib_sonlar[-1]+fib_sonlar[-2])
    return fib_sonlar

print(fib_sonlar_to_n(10))
