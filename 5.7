latin_to_kiril_alphabet = {
    'a': 'а', 'b': 'б', 'c': 'с', 'd': 'д', 'e': 'е', 'f': 'ф', 'g': 'г', 'h': 'ҳ', 'i': 'и', 'j': 'ж',
    'k': 'к', 'l': 'л', 'm': 'м', 'n': 'н', 'o': 'о', 'p': 'п', 'q': 'қ', 'r': 'р', 's': 'с', 't': 'т',
    'u': 'у', 'v': 'в', 'w': 'в', 'x': 'х', 'y': 'й', 'z': 'з'}

def latins_to_kiril(word):
    kirill = ''
    for letter in word.lower():  
        kirill += latin_to_kiril_alphabet.get(letter, letter)
    return kirill

word = input("Lotincha: ")
result = latin_to_kiril_alphabet(word)
print("Kirillcha:", result)

