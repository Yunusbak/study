def sort_dict_by_values(input_dict):
    sorted_dict = {key: input_dict[key] for key in sorted(input_dict, key=input_dict.get)}
    return sorted_dict

print(sort_dict_by_values({'a': 54, 'b': 43, 'c': 5}))

