word = input("So'zni kiriting: ")

letter_counts = {}
for letter in word.lower():
    if letter.isalpha():  
        letter_counts[letter] = letter_counts.get(letter, 0) + 1

kop_ish_soni = max(letter_counts.values())
kam_ish_soni= min(letter_counts.values())
kop_ish = [letter for letter, count in letter_counts.items() if count == kop_ish_soni]
kam_ish = [letter for letter, count in letter_counts.items() if count == kam_ish_soni]

result_dict = {
    "Eng ko'p ishlatilgan harf(lar)": kop_ish,
    "Eng kam ishlatilgan harf(lar)": kam_ish
}

print(result_dict)
